<Page x:Class="FierceStukCloud_PC.MVVM.Views.TestView.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FierceStukCloud_PC.MVVM.Views.TestView"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      KeepAlive="True"
      Title="Page1">

    <Grid Background="White">

        <TextBlock Text="{Binding test, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="30" VerticalAlignment="Center" Foreground="Black"/>
        <Button Command="{Binding update}" VerticalAlignment="Bottom"/>
    </Grid>
</Page>
