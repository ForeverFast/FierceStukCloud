<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:FierceStukCloud_NetCoreLib.Resources.ResourceDictionaries">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/FierceStukCloud_NetCoreLib;component/Resources/ResourceDictionaries/Brushes.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="TBlock_LandP" TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground" Value="{StaticResource StandartTextColor}"/>
        <Setter Property="FontSize" Value="17"/>
        <Setter Property="Background" Value="{x:Null}"/>

        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Margin" Value="0,0,0,10"/>
    </Style>
    
</ResourceDictionary>